      <Binding SchemaName="rh_guide.productid.resco_guide_product.rh_guideid"/>
    </FormTab>
    <FormTab xsi:type="AssociatedTab" flexiHeader="2" NameModified="1" IconModified="0" EntityIconName="MobileCrm\Images.MonoTouch\Tabs\resco_sharepointdoc@2x.png" IsIconNameReadOnly="0">

  <SerializedChildrenV2>
    <FormTab xsi:type="DetailTab" flexiHeader="2">
      <Name>General</Name>
      <SerializedChildren>
        <DetailItem SchemaName="parentcustomerid" Style="Normal"/>
        <DetailItem SchemaName="ownerid"/>
